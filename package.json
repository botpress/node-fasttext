{
  "name": "fasttext",
  "version": "0.2.3",
  "description": "A nodejs binding for fasttext representation and classification",
  "main": "./index.js",
  "scripts": {
    "config": "node-pre-gyp configure --msvs_version=2015",
    "build": "node-pre-gyp rebuild",
    "preinstall": "npm install node-pre-gyp",
    "postinstall": "node-pre-gyp install --fallback-to-build",
    "test": "tape test/start.js | tap-spec"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/botpress/node-fasttext.git"
  },
  "keywords": [
    "vntk",
    "fasttext",
    "node-fasttext"
  ],
  "author": "Botpress, Inc.",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/botpress/node-fasttext/issues"
  },
  "homepage": "https://github.com/botpress/node-fasttext#readme",
  "binary": {
    "module_name": "fasttext",
    "module_path": "./lib/binding/",
    "host": "https://github.com/botpress/node-fasttext/releases/download/",
    "remote_path": "{version}"
  },
  "dependencies": {
    "nan": "2.8.0",
    "node-pre-gyp": "^0.6.39"
  },
  "devDependencies": {
    "@types/node": "^10.12.18",
    "node-pre-gyp-github": "1.3.1",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.0"
  },
  "files": [
    "src",
    "fastText/src",
    "index.js",
    "binding.gyp"
  ]
}
